{
  "name": "clocked",
  "version": "0.1.0",
  "description": "A Internal Dashboard (Refine) project",
  "private": true,
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testMatch='**/tests/unit/**/*.test.{ts,tsx}'",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.{ts,tsx}'",
    "test:e2e": "playwright test",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:a11y": "playwright test --grep @a11y",
    "test:mutation": "stryker run",
    "setup": "playwright install --with-deps",
    "setup:browsers": "playwright install",
    "check:duplication": "jscpd .",
    "check:types": "type-coverage",
    "check:unused": "ts-prune",
    "prepare": "husky install && patch-package",
    "audit": "npm audit --audit-level=moderate",
    "analyze": "ANALYZE=true npm run build",
    "lighthouse": "bash scripts/lighthouse.sh",
    "load-test": "k6 run k6/load-test.js",
    "electron:compile": "tsc -p electron/tsconfig.json",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development npm run electron:compile && cross-env NODE_ENV=development electron .\"",
    "electron:build": "cross-env ELECTRON=true npm run build && npm run electron:compile && electron-builder",
    "electron:start": "npm run electron:compile && electron ."
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@refinedev/cli": "2.16.50",
    "@refinedev/core": "5.0.5",
    "@refinedev/nextjs-router": "7.0.4",
    "@refinedev/react-hook-form": "5.0.2",
    "@refinedev/react-table": "6.0.1",
    "@sentry/nextjs": "10.27.0",
    "@tailwindcss/postcss": "4.1.17",
    "@vercel/analytics": "1.5.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.553.0",
    "next": "16.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "7.66.0",
    "recharts": "3.3.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.17",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@lhci/cli": "0.15.1",
    "@next/bundle-analyzer": "16.1.0",
    "@playwright/test": "1.56.1",
    "@stryker-mutator/core": "9.3.0",
    "@stryker-mutator/jest-runner": "9.3.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "30.0.0",
    "@types/node": "20.19.24",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "dotenv-cli": "11.0.0",
    "electron": "40.0.0",
    "electron-builder": "26.4.0",
    "eslint": "9.39.1",
    "eslint-config-next": "16.1.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-testing-library": "7.13.3",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jscpd": "4.0.5",
    "lint-staged": "16.2.6",
    "patch-package": "8.0.1",
    "playwright": "1.56.1",
    "prettier": "3.6.2",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "ts-prune": "0.10.3",
    "type-coverage": "2.29.7",
    "typescript": "5.9.3",
    "wait-on": "9.0.3"
  },
  "overrides": {
    "glob": "^10.4.5",
    "rimraf": "^5.0.10",
    "inflight": "npm:@nothing/inflight@^1.0.0",
    "tmp": "0.2.5",
    "diff": "^7.0.0",
    "tar": "^7.5.6"
  }
}
